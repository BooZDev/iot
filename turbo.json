{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    }
  },
  "globalEnv": ["SESSION_SECRET_KEY", "BACKEND_URL", "MONGO_URI", "PORT", "MQTT_ULR", "MQTT_USERNAME", "MQTT_PASSWORD", "MQTT_CLIENT_ID_1", "MQTT_CLIENT_ID_2", "JWT_SECRET", "JWT_EXPIRES_IN", "REFRESH_JWT_SECRET", "REFRESH_JWT_EXPIRES_IN"]
}
